{
    "name": "frank/symfonymailer-for-yii2",
    "description": "Symfonymailer for Yii2",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=7.3.19",
        "yiisoft/yii2": ">=2.0.4",
        "symfony/mailer": ">=5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "9.5.10"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "yii\\symfonymailer\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}
